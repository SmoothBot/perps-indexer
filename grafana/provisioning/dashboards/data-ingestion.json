        {
          "datasource": {
            "type": "postgres",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH hourly_data AS (\n  SELECT \n    DATE_TRUNC('hour', timestamp) AS hour,\n    COUNT(*) AS fills_count\n  FROM hl_fills\n  GROUP BY hour\n),\nhour_series AS (\n  SELECT generate_series(\n    (SELECT MIN(hour) FROM hourly_data),\n    (SELECT MAX(hour) FROM hourly_data),\n    '1 hour'::interval\n  ) AS hour\n),\nmissing_hours AS (\n  SELECT \n    hs.hour,\n    COALESCE(hd.fills_count, 0) AS fills_count\n  FROM hour_series hs\n  LEFT JOIN hourly_data hd ON hs.hour = hd.hour\n  WHERE hd.fills_count IS NULL OR hd.fills_count < 100\n)\nSELECT \n  hour AS gap_start,\n  LEAD(hour) OVER (ORDER BY hour) AS gap_end,\n  EXTRACT(EPOCH FROM (LEAD(hour) OVER (ORDER BY hour) - hour)) / 3600 AS gap_hours,\n  fills_count\nFROM missing_hours\nWHERE hour >= NOW() - INTERVAL '30 days'\nORDER BY hour DESC\nLIMIT 100",
          "refId": "A"
        }
      ],
      "title": "Data Gaps (Hours with < 100 fills)",
      "type": "table"
