          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH cumulative_volume AS (\n  SELECT\n    ROW_NUMBER() OVER (ORDER BY total_volume DESC) as rank,\n    SUM(total_volume) OVER (ORDER BY total_volume DESC) as cumulative_vol,\n    SUM(total_volume) OVER () as total_vol\n  FROM trader_summary\n  WHERE exchange_id = (SELECT id FROM exchanges WHERE code = '${exchange}')\n)\nSELECT\n  rank as \"Top N Traders\",\n  (cumulative_vol / NULLIF(total_vol, 0) * 100)::float as \"Volume %\"\nFROM cumulative_volume\nWHERE rank IN (1, 5, 10, 20, 50, 100, 200, 500)\nORDER BY rank",
          "refId": "A"
        }
      ],
      "title": "Market Concentration (Cumulative Volume %)",
      "type": "timeseries"
