          "rawQuery": true,
          "rawSql": "SELECT\n  CONCAT(LEFT(ts.user_address, 6), '...', RIGHT(ts.user_address, 4)) as \"Trader\",\n  ts.user_address as \"Full Address\",\n  ts.total_pnl::numeric as \"Total PnL\",\n  ts.total_volume::numeric as \"Total Volume\",\n  CASE\n    WHEN ts.total_trades > 0 AND ts.total_pnl > 0 THEN 60.0\n    WHEN ts.total_trades > 0 AND ts.total_pnl < 0 THEN 40.0\n    ELSE 50.0\n  END as \"Win Rate\",\n  ts.total_trades as \"Total Trades\",\n  ts.avg_trade_size::numeric as \"Avg Trade Size\"\nFROM trader_summary ts\nWHERE ts.exchange_id = (SELECT id FROM exchanges WHERE code = '${exchange}')\n  AND ('${market}' = 'All' OR EXISTS (\n    SELECT 1 FROM trader_market_summary tms\n    WHERE tms.user_address = ts.user_address\n      AND tms.exchange_id = ts.exchange_id\n      AND tms.symbol = '${market}'\n  ))\nORDER BY ts.total_pnl DESC NULLS LAST\nLIMIT 20",
          "refId": "A"
        }
