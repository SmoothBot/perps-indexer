          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH trader_tiers AS (\n  SELECT\n    user_address,\n    total_volume,\n    avg_trade_size,\n    total_trades,\n    total_pnl,\n    EXTRACT(EPOCH FROM (last_trade_time - first_trade_time)) / 86400.0 as trading_days,\n    CASE\n      WHEN total_volume >= 10000000 THEN 'Mega Whales (>$10M)'\n      WHEN total_volume >= 1000000 THEN 'Whales ($1M-$10M)'\n      WHEN total_volume >= 100000 THEN 'Large Traders ($100K-$1M)'\n      WHEN total_volume >= 10000 THEN 'Medium Traders ($10K-$100K)'\n      ELSE 'Small Traders (<$10K)'\n    END as tier\n  FROM trader_summary\n  WHERE exchange_id = (SELECT id FROM exchanges WHERE code = '${exchange}')\n)\nSELECT\n  tier as \"Trader Tier\",\n  COUNT(*) as \"Trader Count\",\n  AVG(avg_trade_size)::numeric as \"Avg Trade Size\",\n  AVG(CASE WHEN trading_days > 0 THEN total_trades / trading_days ELSE 0 END)::numeric as \"Avg Trades/Day\",\n  SUM(total_pnl)::numeric as \"Total PnL\"\nFROM trader_tiers\nGROUP BY tier\nORDER BY\n  CASE tier\n    WHEN 'Mega Whales (>$10M)' THEN 1\n    WHEN 'Whales ($1M-$10M)' THEN 2\n    WHEN 'Large Traders ($100K-$1M)' THEN 3\n    WHEN 'Medium Traders ($10K-$100K)' THEN 4\n    ELSE 5\n  END",
          "refId": "A"
        }
      ],
      "title": "Trading Behavior by Trader Tier",
      "type": "table"
