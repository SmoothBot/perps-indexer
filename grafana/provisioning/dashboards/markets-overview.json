      "pluginVersion": "11.3.1",
      "targets": [
        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "hl-postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n    m.market_id as \"Market ID\",\n    m.symbol as \"Symbol\",\n    m.market_type as \"Market Type\",\n    COALESCE(m.base_asset, m.market_id) as \"Base Asset\",\n    COALESCE(m.quote_asset, 'USD') as \"Quote Asset\",\n    COUNT(f.id) as \"Total Trades\",\n    COUNT(DISTINCT f.user_address) as \"Unique Traders\",\n    COALESCE(SUM(f.price * f.size), 0)::numeric as \"Total Volume (USD)\",\n    CASE \n        WHEN COUNT(f.id) > 0 THEN (SUM(f.price * f.size) / COUNT(f.id))::numeric\n        ELSE 0\n    END as \"Avg Trade Size (USD)\",\n    MAX(f.timestamp) as \"Last Trade\",\n    CASE \n        WHEN m.is_active THEN 'Active'\n        ELSE 'Inactive'\n    END as \"Status\"\nFROM \n    markets m\nLEFT JOIN \n    fills f ON m.id = f.market_id\nWHERE \n    m.exchange_id = (SELECT id FROM exchanges WHERE name = '${exchange:text}')\n    AND (\n        CASE \n            WHEN '${market_type:text}' = 'All' THEN true\n            ELSE m.market_type = LOWER('${market_type:text}')\n        END\n    )\nGROUP BY \n    m.id, m.market_id, m.symbol, m.market_type, m.base_asset, m.quote_asset, m.is_active\nORDER BY \n    \"Total Volume (USD)\" DESC;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
