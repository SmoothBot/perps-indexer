          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n    m.symbol as \"Market\",\n    COALESCE(SUM(f.price * f.size), 0)::numeric as \"Volume\"\nFROM \n    markets m\nLEFT JOIN \n    fills f ON m.id = f.market_id\nWHERE \n    m.exchange_id = (SELECT id FROM exchanges WHERE name = '${exchange:text}')\n    AND m.is_active = true\n    AND (\n        CASE \n            WHEN '${market_type:text}' = 'All' THEN true\n            ELSE m.market_type = LOWER('${market_type:text}')\n        END\n    )\nGROUP BY \n    m.symbol\nHAVING \n    SUM(f.price * f.size) > 0\nORDER BY \n    \"Volume\" DESC\nLIMIT 20;",
          "refId": "A",
