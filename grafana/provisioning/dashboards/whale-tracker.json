      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH trader_activity AS (\n  SELECT \n    f.user_address,\n    SUM(CASE WHEN f.side = 'BUY' THEN f.price * f.size ELSE 0 END) as total_buy_volume,\n    SUM(CASE WHEN f.side = 'SELL' THEN f.price * f.size ELSE 0 END) as total_sell_volume,\n    SUM(CASE WHEN f.side = 'BUY' THEN f.price * f.size ELSE -f.price * f.size END) as net_volume,\n    COUNT(*) as trade_count,\n    SUM(f.price * f.size) / COUNT(*) as avg_trade_size,\n    SUM(COALESCE(f.closed_pnl, 0)) as total_pnl\n  FROM fills f\n  JOIN markets m ON f.market_id = m.id\n  WHERE \n    ('${coin}' = 'All' OR m.symbol = '${coin}')\n    AND f.exchange_id = (SELECT id FROM exchanges WHERE code = '${exchange}')\n    AND ('${market_type}' = 'All' OR m.market_type = '${market_type}')\n    AND $__timeFilter(f.timestamp)\n  GROUP BY f.user_address\n  HAVING ABS(SUM(CASE WHEN f.side = 'BUY' THEN f.price * f.size ELSE -f.price * f.size END)) > ${min_volume}\n)\nSELECT \n  user_address,\n  net_volume,\n  total_buy_volume,\n  total_sell_volume,\n  trade_count,\n  avg_trade_size,\n  total_pnl\nFROM trader_activity\nORDER BY ABS(net_volume) DESC\nLIMIT 100",
          "refId": "A"
        }
      ],
      "title": "Top Net Buyers/Sellers for ${coin}",
      "type": "table"
