      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH top_traders AS (\n  SELECT \n    f.user_address,\n    ABS(SUM(CASE WHEN f.side = 'BUY' THEN f.price * f.size ELSE -f.price * f.size END)) as abs_net_volume\n  FROM fills f\n  JOIN markets m ON f.market_id = m.id\n  WHERE \n    ('${coin}' = 'All' OR m.symbol = '${coin}')\n    AND f.exchange_id = (SELECT id FROM exchanges WHERE code = '${exchange}')\n    AND ('${market_type}' = 'All' OR m.market_type = '${market_type}')\n    AND $__timeFilter(f.timestamp)\n  GROUP BY f.user_address\n  ORDER BY abs_net_volume DESC\n  LIMIT 10\n)\nSELECT \n  f.timestamp as time,\n  CONCAT(LEFT(f.user_address, 6), '...', RIGHT(f.user_address, 4)) as metric,\n  SUM(SUM(CASE WHEN f.side = 'BUY' THEN f.price * f.size ELSE -f.price * f.size END)) OVER (PARTITION BY f.user_address ORDER BY f.timestamp) as value\nFROM fills f\nJOIN markets m ON f.market_id = m.id\nJOIN top_traders t ON f.user_address = t.user_address\nWHERE \n  ('${coin}' = 'All' OR m.symbol = '${coin}')\n  AND f.exchange_id = (SELECT id FROM exchanges WHERE code = '${exchange}')\n  AND ('${market_type}' = 'All' OR m.market_type = '${market_type}')\n  AND $__timeFilter(f.timestamp)\nGROUP BY f.timestamp, f.user_address\nORDER BY f.timestamp",
          "refId": "A"
        }
      ],
      "title": "Cumulative Net Position Over Time - Top 10 Traders",
